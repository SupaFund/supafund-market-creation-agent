{
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/main.py"
    }
  ],
  "functions": {
    "api/main.py": {
      "maxDuration": 300,
      "memory": 3008
    }
  },
  "env": {
    "PYTHONPATH": "/var/task:/var/task/gnosis_predict_market_tool"
  }
}